{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": ["**/*.{ts,js}"],
  "entry": ["**/*.{config,cfg}.{ts,js}", "src/main.ts", "src/**/*-main.ts"],
  "ignore": [
    "node_modules",
    "dist-jsr",
    "dist-npm",
    "dist-libs",
    "tests/**/*.test.ts",
    "src/types.ts"
  ],
  "ignoreBinaries": ["relidler"]
}
